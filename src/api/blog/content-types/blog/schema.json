{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "thumbnail": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "content": {
      "type": "blocks"
    },
    "team": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::team.team",
      "inversedBy": "blogs"
    },
    "isFeatured": {
      "type": "boolean"
    }
  }
}
